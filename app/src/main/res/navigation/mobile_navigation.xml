        android:name="com.duvarax.navigationactivity.ui.cargar.CargarFragment"
        android:label="Cargar"
        tools:layout="@layout/fragment_cargar" />

    <fragment
        android:id="@+id/nav_salir"
        android:name="com.duvarax.navigationactivity.ui.salir.SalirFragment"
        android:label="Salir"
